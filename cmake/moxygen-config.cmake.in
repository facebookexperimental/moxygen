# This module sets the following variables:
#   moxygen_FOUND
#   moxygen_INCLUDE_DIRS
#   moxygen_LIBRARIES

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(folly)
find_dependency(wangle)
find_dependency(mvfst)
find_dependency(proxygen)
find_dependency(Glog)

if(NOT TARGET moxygen::moxygen_moq)
    include("${CMAKE_CURRENT_LIST_DIR}/moxygen-targets.cmake")
    get_target_property(moxygen_INCLUDE_DIRS moxygen::moxygen_moq INTERFACE_INCLUDE_DIRECTORIES)
endif()

if(NOT moxygen_FIND_QUIETLY)
    message(STATUS "Found moxygen: ${PACKAGE_PREFIX_DIR}")
endif()

set(moxygen_LIBRARIES
  moxygen::moxygen_moq
  moxygen::moxygen_moq_server
  moxygen::moxygen_moq_relay_session
  moxygen::moxygen_relay_moq_relay
  moxygen::moxygen_relay_moq_relay_client
  moxygen::moxygen_moqclient
)
